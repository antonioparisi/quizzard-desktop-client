<div class='col-lg-12 status start'>
  <div class='col-lg-12 question-wrapper animated fadeInDown'>
    <div class='question-status'>
      <div class='mini-logo'>
        <p class='timer'>{{timerCurrentQuestion.seconds}}</p>
      </div>
    </div>
    <div class='panel panel-default'>
      <div class='panel-body'>
        <p class='questions-status'>Question {{questionStatusFormat}} of {{totalQuestions}}</p>
        <hr>
        <p class='title'>{{currentQuestion.title}}</p>
      </div>
    </div>
  </div>
</div>
